<?xml version='1.0' encoding="UTF-8"?>

<ElementSupportEventList>
    <ElementSupportEvent name="PushButton" ShowName="弹出按钮">
        <Event name="Press" ShowName="按下事件">
            <desc>弹出按钮被按下时触发本事件</desc>
        </Event>
        <Event name="Release" ShowName="抬起事件">
            <desc>弹出按钮从按下至抬起时触发本事件</desc>
        </Event>
    </ElementSupportEvent>
    <ElementSupportEvent name="SwitchButton" ShowName="切换按钮">
        <Event name="Press" ShowName="切换到开事件">
            <desc>切换按钮由关状态切换至开状态时触发本事件</desc>
        </Event>
        <Event name="Release" ShowName="切换到关事件">
            <desc>切换按钮由开状态切换至关状态时触发本事件</desc>
        </Event>
    </ElementSupportEvent>	
	<ElementSupportEvent name="Clock" ShowName="时钟">
        <Event name="TimeOut" ShowName="时钟到来">
            <desc>时钟滴答达到时钟间隔时触发本事件</desc>
        </Event>
    </ElementSupportEvent>
    <ElementSupportEvent name="GraphPage" ShowName="画面">
        <Event name="Press" ShowName="打开画面">
            <desc>打开画面时触发本事件</desc>
        </Event>
        <Event name="Release" ShowName="关闭画面">
            <desc>关闭画面时触发本事件</desc>
        </Event>
    </ElementSupportEvent>
</ElementSupportEventList>
